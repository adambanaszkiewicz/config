{
    "name": "requtize/config",
    "description": "Simple Config library, that handle PHP arrays, YAML and INI files as configs and can dump all configs into one PHP file as Cache.",
    "keywords": ["config", "conf", "configuration", "settings", "setting", "php"],
    "license": "MIT",
    "version": "1.2.0",
    "homepage": "https://github.com/requtize/config",
    "authors": [
        {
            "name": "Adam Banaszkiewicz",
            "homepage": "https://github.com/requtize",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5 || ~7.1",
        "requtize/fresh-file": "~0.3",
        "symfony/yaml": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Requtize\\Config\\": "src/Config/"
        }
    }
}
